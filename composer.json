{
  "name": "progamer_anyaran/php-login",
  "description": "PHP Login Management by : Progamer Anyaran",
  "type": "project",
  "authors": [
    {
      "name": "progamer-anyaran",
      "email": "imachmud97@gmail.com"
    }
  ],
  "require": {
    "php": ">=8",
    "twbs/bootstrap": "v5.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "10.3.5"
  },
  "autoload": {
    "psr-4": {
      "PROGAMERANYARAN\\PHP\\LOGIN\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PROGAMERANYARAN\\PHP\\LOGIN\\": "tests"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "rm -rf public/bootstrap",
      "cp -R vendor/twbs/bootstrap/dist public/bootstrap"
    ]
  }
}
